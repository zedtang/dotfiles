[user]
    name = Jiaheng Tang
    email = tjiaheng@vmware.com
[push]
    default = current
[diff]
    tool = vimdiff
[difftool]
    prompt = false
[merge]
    tool = vimdiff
[mergetool]
    prompt = false
    keepBackup = false
[core]
    pager = delta
[interactive]
    diffFilter = delta --color-only
[delta]
    side-by-side = true
    syntax-theme = GitHub
[rebase]
    autosquash = true
